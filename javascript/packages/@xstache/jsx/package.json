{
    "name": "@xstache/jsx",
    "version": "0.0.2",
    "description": "Compile Xstache templates to JSX components",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dirk/xstache.git"
    },
    "type": "module",
    "bin": {
        "xstache-jsx": "dist/cli.js"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rimraf dist && tsc",
        "test": "vitest --run"
    },
    "devDependencies": {
        "@types/node": "^22",
        "@types/yargs": "^17",
        "@vitest/coverage-v8": "catalog:",
        "react": "^19",
        "react-dom": "^19",
        "rimraf": "catalog:",
        "source-map": "^0",
        "tmp-promise": "^3",
        "typescript": "catalog:",
        "vitest": "catalog:",
        "zx": "^8"
    },
    "peerDependencies": {
        "@xstache/ast": "workspace:*",
        "@xstache/cli": "workspace:*",
        "@xstache/jsx-runtime": "workspace:*",
        "@xstache/parse": "workspace:*"
    },
    "dependencies": {
        "@babel/types": "^7",
        "astring": "^1"
    }
}
